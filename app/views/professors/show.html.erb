<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @professor.title %>
  <%= @professor.first_name %>
  <%= @professor.last_name %>
</p>
<p>
  <strong>Department:</strong>
  <%= @professor.department %>
</p>

<ul>
  <% @professor.courses.each do |course| %>
    <li><%= course %></li>
  <% end %>
</ul>

<%= link_to 'Edit', edit_professor_path(@professor) %> |
<%= link_to 'Back', professors_path %>
